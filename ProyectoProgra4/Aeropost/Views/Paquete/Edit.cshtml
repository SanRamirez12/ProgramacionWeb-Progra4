@model Aeropost.Models.Paquete

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Paquete</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Peso" class="control-label"></label>
                <input asp-for="Peso" class="form-control" />
                <span asp-validation-for="Peso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ValorTotalBruto" class="control-label"></label>
                <input asp-for="ValorTotalBruto" class="form-control" />
                <span asp-validation-for="ValorTotalBruto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TiendaOrigen" class="control-label"></label>
                <input asp-for="TiendaOrigen" class="form-control" />
                <span asp-validation-for="TiendaOrigen" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="CondicionEspecial" /> @Html.DisplayNameFor(model => model.CondicionEspecial)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="FechaRegistro" class="control-label"></label>
                <input asp-for="FechaRegistro" class="form-control" />
                <span asp-validation-for="FechaRegistro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClienteAsociado" class="control-label"></label>
                <input asp-for="ClienteAsociado" class="form-control" />
                <span asp-validation-for="ClienteAsociado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

